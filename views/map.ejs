	<script>
	
	
	
	/** test with 
	http://localhost:5000/secure_map?userfilepresent=true&userfile=http://localhost:5000/sggeo.json
	**/
	
	function getURLParameter(name) {
	  return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null
	}

	window.onload = function(){
		
		if(getURLParameter('userfilepresent') != null){
			var apiAddress = getURLParameter('userfile');
            var jsonData = null;
            alert("Address: "+apiAddress);
           /**
            $.getJSON(apiAddress,function(data){
              jsonData = data;
              alert(JSON.stringify(jsonData));
              });
            **/
            loadUserUploads(apiAddress);
            //MAP.loadVenuesChoroplethLayer(input,response,groupColor);
            //displayUserPoints(apiAddress);
		}
	};
	
	
